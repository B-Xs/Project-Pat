-- Anti Concat Dump
local function Punish(...)
    while true do end
    end

local UselessAlphabet = {'A','B','C','D','CBA'}

local Shit = {table.concat}
   if debug.getinfo(Shit[(2-1)]).what ~= UselessAlphabet[3] then
       C1 = false
           end

if C1 == false then Punish() end  


-- Check functions are not changed? example loadstring
local SuperCoolBlacklist = {print,error,warn,writefile}

for e,n in pairs(SuperCoolBlacklist)
                do 
    if loadstring == n then Punish() 
                end
            end  


